</head>

<body>
    <button onclick="location.href = '/logout';" id="logout" class="btn btn-danger ml-3 logout-res">Logout</button>

    <nav class="navbar fixed-top navbar-light justify-content-between" style="background-color: #404243 ;">
        
        <a class="navbar-brand text-white">Search Results</a>
        <div class="search-normal">
            <form class="form-inline" method="POST" action='/search'>
            <input name="Search" class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
            <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>

            </form>
            <button onclick="location.href = '/logout';" id="logout" class="btn btn-danger ml-3">Logout</button>

        </div>    
      </nav>
      
    <br>

    <div class="container search-res">
        <form class="form-inline" method="POST" action='/search'>
            <input name="Search" class="form-control col-md-9" type="search" placeholder="Search" aria-label="Search">
            <button class="btn btn-outline-success my-2 col-md-3" type="submit">Search</button>

        </form>
    </div>
    <br>
    <br>
    
    <div class="links">
        <% for (let result of list){ %>
            <a class="link" href="<%=result.destination%>">&#x2022; &nbsp; <%=result.destination%></a>
            <br>
        <% } %>    
    </div>
